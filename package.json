{
  "name": "eslint-config-logux",
  "version": "7.0.0",
  "description": "An ESLint shareable config for Logux",
  "keywords": [
    "logux",
    "eslint",
    "eslint-config"
  ],
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "logux/eslint-config-logux",
  "peerDependencies": {
    "eslint": "^3.16.1",
    "eslint-config-standard": "^7.0.0",
    "eslint-plugin-jest": "^19.0.1",
    "eslint-plugin-node": "^4.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1"
  },
  "devDependencies": {
    "eslint": "^3.16.1",
    "eslint-config-standard": "^7.0.0",
    "eslint-plugin-jest": "^19.0.1",
    "eslint-plugin-node": "^4.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1",
    "lint-staged": "^3.3.1",
    "pre-commit": "^1.1.3",
    "yaspeller-ci": "^0.1.0"
  },
  "scripts": {
    "lint-staged": "lint-staged",
    "spellcheck": "yaspeller-ci *.md",
    "lint": "eslint *.js",
    "test": "npm run lint && npm run spellcheck"
  },
  "eslintConfig": {
    "extends": "./node4.js"
  },
  "lint-staged": {
    "*.md": "yaspeller-ci",
    "*.js": "eslint"
  },
  "pre-commit": [
    "lint-staged"
  ]
}
