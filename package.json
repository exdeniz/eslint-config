{
  "name": "@logux/eslint-config",
  "version": "27.0.0",
  "description": "An ESLint shareable config for Logux",
  "keywords": [
    "logux",
    "eslint",
    "eslint-config"
  ],
  "scripts": {
    "spellcheck": "yaspeller-ci *.md",
    "lint": "eslint *.js",
    "test": "npm run lint && npm run spellcheck"
  },
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "logux/config-logux",
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "globals": "^11.9.0"
  },
  "peerDependencies": {
    "eslint": "^5.8.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.0.1",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.0"
  },
  "devDependencies": {
    "clean-publish": "^1.1.0",
    "eslint": "^5.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.0.1",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-prefer-let": "^1.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "yaspeller-ci": "^1.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.md": "yaspeller-ci",
    "*.js": "eslint"
  },
  "eslintConfig": {
    "extends": "./node.js"
  },
  "yaspeller": {
    "lang": "en",
    "ignoreCapitalization": true,
    "dictionary": [
      "Logux ESLint",
      "ESLint",
      "Logux",
      "Versioning",
      "ES2015",
      "Nikita",
      "Gusakov",
      "Fursov",
      "js"
    ]
  }
}
