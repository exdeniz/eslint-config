{
  "name": "@logux/eslint-config",
  "version": "40.0.2",
  "description": "An ESLint shareable config for Logux",
  "keywords": [
    "logux",
    "eslint",
    "eslint-config"
  ],
  "scripts": {
    "test": "eslint . && ./check-peer.js && yaspeller *.md"
  },
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "logux/eslint-config",
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "globals": "^13.2.0"
  },
  "peerDependencies": {
    "eslint": "^7.6.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-let": "^1.0.2",
    "eslint-plugin-prettierx": "^0.14.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^21.0.0"
  },
  "devDependencies": {
    "@logux/sharec-config": "^0.7.4",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "clean-publish": "^1.1.8",
    "eslint": "^7.6.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-let": "^1.0.2",
    "eslint-plugin-prettierx": "^0.14.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^21.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "typescript": "^3.9.7",
    "yaspeller": "^7.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.md": "yaspeller",
    "*.js": "eslint --fix",
    "package.json": "./check-peer.js"
  },
  "eslintConfig": {
    "extends": "./ts"
  },
  "yaspeller": {
    "lang": "en",
    "ignoreCapitalization": true,
    "ignoreText": [
      " \\(by [^)]+\\)."
    ],
    "dictionary": [
      "Logux ESLint",
      "ESLint",
      "Logux",
      "Versioning",
      "ES2015",
      "js",
      "configs",
      "ES6",
      "JSX"
    ]
  },
  "sharec": {
    "config": "@logux/sharec-config",
    "version": "0.7.4"
  }
}
